{
  "name": "windowed-observable",
  "version": "0.3.4",
  "description": "Simple messaging api",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prebuild": "rm -Rf dist/",
    "build": "tsc",
    "prepare": "yarn build",
    "validate": "npm-run-all --parallel lint test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luistak/windowed-observable.git"
  },
  "author": "luis.takahashi",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/luistak/windowed-observable/issues"
  },
  "homepage": "https://github.com/luistak/windowed-observable#readme",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "dist",
      "node_modules"
    ]
  }
}
